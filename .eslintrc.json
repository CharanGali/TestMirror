{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-empty-interface": ["off"],
	        "@typescript-eslint/no-var-requires": 0

    }
  }